      nombre={el.nombre}\n            calificacion={el.calificacion}\n            autor={el.autor}\n            img = {el.img}\n            categoria = {el.categoria}\n            reseña = {el.reseña}\n            id={el.id}\n            valor = {el.valor}\n            stock= {el.stock}\n\n            />\n          ))}\n        \n      \n\n\n        \n        </div>\n\n    );\n     \n\n  /* JSX que devuelva un ItemDetail (punto 2) */\n}\n\nexport default ItemDetailContainer;\n","/home/german/Documentos/CODERHOUSE/React/preEntrega2Soto/src/Layout/NavBar.js",["161","162"],[],"/home/german/Documentos/CODERHOUSE/React/preEntrega2Soto/src/Components/ItemlistContainer.js",[],[],"/home/german/Documentos/CODERHOUSE/React/preEntrega2Soto/src/Components/Spinner.js",[],[],"/home/german/Documentos/CODERHOUSE/React/preEntrega2Soto/src/Components/ItemDetail.js",["163"],[],"/home/german/Documentos/CODERHOUSE/React/preEntrega2Soto/src/Components/ItemList.js",[],[],"/home/german/Documentos/CODERHOUSE/React/preEntrega2Soto/src/Components/CartWidget.js",["164","165"],[],"/home/german/Documentos/CODERHOUSE/React/preEntrega2Soto/src/Components/ItemCount.js",["166"],[],"import React from 'react'\nimport { useState, useEffect } from 'react'\n\n\nexport default function ItemCount({ stock, initial, funcion, alt }) {\n    const [value, setValue] = useState(initial);\n\n    function onAdd() {\n        (stock) > value ? setValue(value + 1) : setValue(value + 0);\n    }\n\n    function onSubstract() {\n        value !== 0 ? setValue(value - 1) : setValue(value + 0);\n    }\n\n    useEffect(() => {\n        funcion(value);\n      \n    }, [value])\n    \n    \n    return (\n        <div className={alt}>\n            <div className='flex justify-center my-4'>\n                <button className=\"fill-white h-8 w-7 p-1 hover:bg-slate-700 rounded-full mx-3\" onClick={onSubstract}><svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 448 512\"><path d=\"M432 256c0 17.7-14.3 32-32 32L48 288c-17.7 0